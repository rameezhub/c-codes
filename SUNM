#include <stdio.h>

#define SIZE 5 // Define the size of the arrays

int main() {
    int array1[3], array2[3], sumArray[3];

    // Input elements for the first array
    printf("Enter %d elements for the first array:\n", SIZE);
    for (int i = 0; i < 3; i++) {
        printf("Element %d: ", i + 1);
        scanf("%d", &array1[i]);
    }

    // Input elements for the second array
    printf("Enter %d elements for the second array:\n", SIZE);
    for (int j = 0; j < 3; j++) {
        printf("Element %d: ", j + 1);
        scanf("%d", &array2[j]);
    }

    // Sum the corresponding elements of both arrays
    for (int i = 0; i < 3; i++) 
     for(int j= 0;j < 3; j++)
    {
        sumArray[i] = array1[i] + array2[j];
   }

    // Print the resulting sum array
    printf("Sum of the two arrays:\n");
   for (int i = 0; i < 3; i++) 
   for(int j= 0;j < 3; j++)
   {
        printf("%d ", sumArray[i]);
    }
    printf("\n");

    return 0;
